print('Enter a 1st number: ')
num1 = input()
print('Enter a 2nd number: ')
num2 = input()

print('The sum of numbers one and two is ' + str(int(num1) + int(num2)))
print('The difference of numbers one and two is ' + str(int(num1) + int(num2)))
print('The product of numbers one and two is ' + str(int(num1) + int(num2)))
print('The quotient of numbers one and two is ' + str(int(num1) + int(num2)))
